import java.util.*;
public class Main
{
	public static void main(String[] args) {
		Scanner obj=new Scanner(System.in);
		int num1=obj.nextInt();
		int num2=obj.nextInt();
		int count=0;
		while(num1!=0 || num2!=0)
		{
		    int ans=0;
		    int n=num1%10;
		    int m=num2%10;
		    num1=num1/10;
		    num2=num2/10;
		    
		    ans=n+m;
		    if(ans>9)
		    count++;
		}
		System.out.println(count);
	}
}
